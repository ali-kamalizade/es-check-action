{
  "name": "es-check-action",
  "version": "1.0.0",
  "description": "GitHub Action to check JavaScript files against a specified version of ECMAScript (ES)",
  "main": "dist/index.js",
  "private": true,
  "scripts": {
    "build": "ncc build src/index.ts --minify",
    "build:watch": "npm run build -- --watch",
    "ci": "npm run build && npm run test",
    "start": "node dist/index.js",
    "test": "jest",
    "test:coverage": "npm run test -- --coverage",
    "test:watch": "npm run test -- --watch"
  },
  "dependencies": {
    "@actions/core": "^1.2.5"
  },
  "devDependencies": {
    "@types/node": "^14.6.4",
    "@types/jest": "^26.0.13",
    "@vercel/ncc": "^0.24.0",
    "es-check": "^5.1.0",
    "jest": "^26.4.2",
    "ts-jest": "^26.3.0",
    "typescript": "^4.0.2"
  },
  "keywords": [
    "GitHub",
    "Action",
    "JavaScript",
    "es-check",
    "bundling",
    "es"
  ],
  "author": "Ali Kamalizade",
  "bugs": {
    "url": "https://github.com/ali-kamalizade/es-check-action/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ali-kamalizade/es-check-action"
  },
  "license": "MIT"
}
